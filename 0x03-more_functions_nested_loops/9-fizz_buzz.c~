#include <unistd.h>
#include <stdlib.h>
#include <stdio.h>
/**
 * main - fizzbuzz mawfks
 *
 * Return: Always zero
 */
int main(void)
{
	int i;

	for (i = 0; i < 101; i++)
	{
		if (i % 3 == 0 && i % 5 == 0)
			printf("FizzBuzz");
		else if (i % 3 == 0 && i % 5 != 0)
			printf("Fizz");
		else if (i % 3 != 0 && i % 5 == 0)
			printf("Buzz");
		else
			printf("%d", i);
		if (i != 100)
			printf(" ");
		else
			printf("\n");
	}
	return (0);
}
