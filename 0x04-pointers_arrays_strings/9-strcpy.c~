#include "holberton.h"

/**
 * *_strcpy - copies string and prints it
 * @dest: points to the buffer
 * @src: points to a string that we need to copy
 * Return: pointer to dest
 */
char *_strcpy(char *dest, char *src)
{
	int i;

	i = 0;
	while (src[i - 1])
	{
		dest[i] = src[i];
		i++;
	}
	return (dest);
}
